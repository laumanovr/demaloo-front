{
  "name": "front-demaloo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build_staging": "vue-cli-service build --mode staging",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "core-js": "^3.19.1",
    "date-fns": "^2.25.0",
    "exceljs": "^4.3.0",
    "file-saver": "^2.0.5",
    "firebase": "^9.2.0",
    "socket.io-client": "^4.3.2",
    "vue": "^2.6.14",
    "vue-carousel": "^0.18.0",
    "vue-gtag": "^1.16.1",
    "vue-i18n": "^8.26.7",
    "vue-inline-svg": "^2.1.0",
    "vue-js-modal": "^2.0.1",
    "vue-loader": "^15.9.8",
    "vue-masked-input": "^0.5.2",
    "vue-router": "^3.5.3",
    "vue-toast-notification": "^0.6.1",
    "vue2-timepicker": "^1.1.6",
    "vuetify": "^2.5.10",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.15",
    "@vue/cli-plugin-eslint": "~4.5.15",
    "@vue/cli-plugin-router": "~4.5.15",
    "@vue/cli-plugin-vuex": "~4.5.15",
    "@vue/cli-service": "~4.5.15",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "mini-css-extract-plugin": "^2.4.3",
    "node-sass": "^6.0.1",
    "sass": "~1.32.7",
    "sass-loader": "^10.0.0",
    "style-resources-loader": "^1.4.1",
    "vue-cli-plugin-style-resources-loader": "~0.1.5",
    "vue-cli-plugin-vuetify": "~2.4.3",
    "vue-template-compiler": "^2.6.14",
    "vuetify-loader": "^1.7.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-console": "off",
      "no-debugger": "off",
      "template-curly-spacing": "off",
      "array-bracket-spacing": [
        "warn",
        "never"
      ],
      "object-curly-spacing": [
        "warn",
        "never"
      ],
      "semi": [
        "warn",
        "always"
      ],
      "indent": [
        "warn",
        "tab",
        {
          "ignoredNodes": [
            "TemplateLiteral"
          ]
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
