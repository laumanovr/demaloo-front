{
  "name": "front-demaloo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build_staging": "vue-cli-service build --mode staging",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "date-fns": "^2.19.0",
    "exceljs": "^4.2.1",
    "file-saver": "^2.0.5",
    "firebase": "^8.2.10",
    "moment": "^2.29.1",
    "vue": "^2.6.11",
    "vue-carousel": "^0.18.0",
    "vue-js-modal": "^2.0.0-rc.6",
    "vue-masked-input": "^0.5.2",
    "vue-router": "^3.2.0",
    "vue-toast-notification": "^0.6.1",
    "vue2-timepicker": "^1.1.6",
    "vuetify": "^2.4.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss7-compat": "^2.0.4",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.12.0",
    "postcss": "^7.0.35",
    "sass": "^1.32.7",
    "sass-loader": "^10.0.0",
    "style-resources-loader": "^1.3.2",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.4",
    "vue-cli-plugin-style-resources-loader": "~0.1.4",
    "vue-cli-plugin-vuetify": "~2.1.0",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.7.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-console": "off",
      "no-debugger": "off",
      "template-curly-spacing": "off",
      "array-bracket-spacing": [
        "warn",
        "never"
      ],
      "object-curly-spacing": [
        "warn",
        "never"
      ],
      "semi": [
        "warn",
        "always"
      ],
      "indent": [
        "warn",
        "tab",
        {
          "ignoredNodes": [
            "TemplateLiteral"
          ]
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
